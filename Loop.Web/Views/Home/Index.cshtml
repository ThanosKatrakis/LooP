        <!-- ======= About Us Section ======= -->
        <section id="about" class="about">
            <div class="container" data-aos="fade-up">

                <div class="section-title">
                    <h2>About Us</h2>
                </div>

                <div class="row content">
                    <div class="col-lg-6">
                        <p>
                            It's time to make a <b>stronger coding community.</b>
                            Loop is place where you can talk with fellow developers about coding and technology.
                            In our site you can also buy:
                        </p>
                        <ul>
                            <li><i class="ri-check-double-line"></i> Video tutorials for all technologies you can name.</li>
                            <li><i class="ri-check-double-line"></i> Book tutorials for all modern coding languages.</li>
                            <li><i class="ri-check-double-line"></i> And all that at the the price you <b>choose to pay.</b></li>
                        </ul>
                    </div>
                    <div class="col-lg-6 pt-4 pt-lg-0">
                        <p>
                            Our mission is to <b>help</b> developers write the script of the future.
                            This means helping you find and hire skilled developers for your business and providing them the tools they need to <b>share knowledge</b> and work effectively.
                        </p>
                        <a href="#team" class="btn-learn-more">Learn More</a>
                    </div>
                </div>
            </div>

        </section><!-- End About Us Section -->
        <!-- ======= Why Us Section ======= -->
        <section id="why-us" class="why-us section-bg">
            <div class="container-fluid" data-aos="fade-up">
                <div class="row">
                    <div class="col-lg-7 d-flex flex-column justify-content-center align-items-stretch  order-2 order-lg-1">
                        <div class="content">
                            <h3>Our latest <strong>Posts</strong></h3>
                            <p>
                                You can find our latest answers to your problems posted by Loop users.
                                Click @Html.ActionLink("Here", "Index", "Post")
                            </p>
                        </div>

                        <div class="accordion-list">
                            <ul>
                                @{
                                    var counter = 1;
                                }
                                @foreach (var post in Model.RecentPosts.ToList())
                                {
                                    <li>
                                        <a data-toggle="collapse" class="collapse" href="#accordion-list-@counter.ToString()"><span>0 @counter</span> @post.Title <i class="bx bx-chevron-down icon-show"></i><i class="bx bx-chevron-up icon-close"></i></a>
                                        <div id="accordion-list-@counter.ToString()" class="collapse" data-parent=".accordion-list">
                                            <p>
                                                @post.Text
                                            </p>
                                        </div>
                                    </li>
                                    counter++;
                                }
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-5 align-items-stretch order-1 order-lg-2 img" style="background-image: url('/Content/assets/why-us.png');" data-aos="zoom-in" data-aos-delay="150">
                        &nbsp;
                    </div>
                </div>
            </div>
        </section><!-- End Why Us Section -->
        <!-- ======= Skills Section ======= -->
        <section id="skills" class="skills">
            <div class="container" data-aos="fade-up">

                <div class="row">
                    <div class="col-lg-6 d-flex align-items-center" data-aos="fade-right" data-aos-delay="100">
